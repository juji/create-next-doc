#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/juji/play/create-next-doc/bin/node_modules:/Users/juji/play/create-next-doc/node_modules:/Users/juji/play/node_modules:/Users/juji/node_modules:/Users/node_modules:/node_modules:/Users/juji/play/create-next-doc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/juji/play/create-next-doc/bin/node_modules:/Users/juji/play/create-next-doc/node_modules:/Users/juji/play/node_modules:/Users/juji/node_modules:/Users/node_modules:/node_modules:/Users/juji/play/create-next-doc/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/install.mjs" "$@"
else
  exec node  "$basedir/../../bin/install.mjs" "$@"
fi
